FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    "(left_1|bottom_1|right_3|left_4|right_4|bottom_3)"
    {
        //type            fixedValue;
        value           uniform 0;
        type            fixedFluxPressure;
    }

    bottom_4    
    {
        type            fixedValue;
        value           uniform 0;
    }
    "(top_1|top_2|top_3|top_4|bottom_2|right_1|left_2|right_2|left_3)"
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1;
        value           uniform 0;
    }
    movingObject
    {
        type            fixedFluxPressure;
    }
}

