FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("liboverset.so");

application     overInterDyMFoam ;

startFrom       latestTime;
startTime       0.0;
stopAt          endTime;
endTime         3;//0.05
deltaT          0.00005;
writeControl    adjustableRunTime;
writeInterval   0.00005;//0.01;
purgeWrite      0;
writeFormat     ascii;
writePrecision  7;
writeCompression yes;
timeFormat      general;
timePrecision   6;
runTimeModifiable yes;
adjustTimeStep  yes;

maxCo           0.9;//1;
maxAlphaCo      0.9;//1;
maxDeltaT       1;

