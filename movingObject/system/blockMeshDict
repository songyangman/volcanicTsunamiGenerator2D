FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
scale   1;

X0	-0.75;
Y0	-1.75;
Z0	-0.01;

X1	0.75;
Y1	0.85;
Z1	0.01;

nx	30;//18;
ny	52;//46;
nz	1;


vertices
(
    ($X0 $Y0 $Z0)
    ($X1 $Y0 $Z0)
    ($X1 $Y1 $Z0)
    ($X0 $Y1 $Z0)
    ($X0 $Y0 $Z1)
    ($X1 $Y0 $Z1)
    ($X1 $Y1 $Z1)
    ($X0 $Y1 $Z1)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) movingZone ($nx $ny $nz) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    oversetPatch
    {
        type overset;
        faces
        (
            (2 6 5 1)
            (0 4 7 3)
            (1 5 4 0)
            (2 3 7 6)

        );
    }
    frontAndBack
    {
        type empty;
        faces
        (
            (0 3 2 1) //
            (4 5 6 7) //
        );
    }
    // to be filled by subsetMesh
    movingObject
    {
        type wall;
        faces ();
    }

);

